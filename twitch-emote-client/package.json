{
  "name": "twitch-emote-client",
  "version": "0.2.0",
  "description": "a twitch and third-party emote client",
  "license": "MIT",
  "repository": {
    "directory": "twitch-emote-client",
    "url": "https://github.com/Juliapixel/twitch_emote_api"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/type/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "preversion": "npm run build",
    "format": "prettier -w ."
  },
  "keywords": [],
  "author": "Juliapixel",
  "devDependencies": {
    "@types/three": "^0.168.0",
    "@types/tmi-js": "npm:@types/tmi.js@^1.8.6",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "three": ">= 0.160"
  },
  "dependencies": {
    "tmi-js": "npm:tmi.js@^1.8.5"
  }
}
